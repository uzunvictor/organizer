{
    "env": {
        "browser": true,
        "es2017": true
    },
    "extends": "plugin:vue/essential",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/multi-word-component-names": "off"
    }
}